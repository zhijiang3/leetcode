{
  "compilerOptions": {
    "pretty": true,
    "downlevelIteration": true,
    "importHelpers": true,
    "moduleResolution": "node",
    "lib": [
      "ES5",
      "ES2015.Core",
      "ES2015.Symbol",
      "ES2015.Promise",
      "ES2015.Collection",
      "ES2015.Iterable"
    ],
    "baseUrl": ".",
    "paths": {
      "shared/*": ["./shared/*"],
      "data-structure/*": ["./data-structure/*"],
      "problems/*": ["./problems/*"]
    }
  },
  "include": ["problems", "data-structure", "shared"]
}